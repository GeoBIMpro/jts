<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google, Inc.//DTD GWT Module Descriptor//EN" "https://github.com/gwtproject/gwt/raw/2.6.1/distro-source/core/src/gwt-module.dtd">
<module rename-to="JTSWebApp">
	<!-- Enable logging first -->
	<inherits name="com.google.gwt.logging.Logging" />
	<set-property name="gwt.logging.enabled" value="TRUE"/>
	<set-property name="gwt.logging.logLevel" value="FINEST" />
	<set-property name="gwt.logging.hasWidgetsHandler" value="ENABLED" />
	
	<!-- GWT modules -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.xml.XML" />
	
	<!-- Other modules -->
	<inherits name="com.vividsolutions.JTS"/>
		
	<entry-point class="com.vividsolutions.jtswebapp.client.JTSWebAppEntry" />
	
	<!-- Default warning for non-static, final fields enabled -->
	<set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="true" />
	
	<source path="client" />
	<source path="shared" />
</module>